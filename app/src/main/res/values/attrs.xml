<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyCustomView">
        <attr name="piv_shape_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ArtIndicator">
        <attr name="mi_radius_selected" format="dimension" />
        <attr name="mi_radius_unselected" format="dimension" />

        <attr name="mi_color_selected" format="color" />
        <attr name="mi_color_unselected" format="color" />

        <attr name="mi_distance_dot" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="EmotionalFaceView">
        <attr name="faceColor" format="color" />
        <attr name="eyesColor" format="color" />
        <attr name="mouthColor" format="color" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="state" format="enum">
            <enum name="happy" value="0" />
            <enum name="sad" value="1" />
        </attr>
    </declare-styleable>
</resources>
